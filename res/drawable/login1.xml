<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape>

            <!-- //边缘颜色   按钮四个圆角="" -->
            <corners android:radius="0.01dip" />

            <stroke android:width="30dp" android:color="#FFE947" />
        </shape></item>
    <item android:state_pressed="false"><shape>

            <!-- //边缘颜色   按钮四个圆角="" -->
            <corners android:radius="0.01dip" />

            <stroke android:width="30dp" android:color="#44AEFF" />
        </shape></item>

</selector>